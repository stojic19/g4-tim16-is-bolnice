            <Border Margin="10,616,662,61">
                <Path Stroke="Black" StrokeThickness="1" Fill="Cyan" Stretch="Fill" Margin="0,4,-22,6">
                    <Path.Data>
                        <RectangleGeometry
            Rect="20,20,150,50" RadiusX="3" RadiusY="3" />
                    </Path.Data>
                </Path>
            </Border>
            <Label Content="-Lični nalog" FontSize="20" HorizontalAlignment="Left" VerticalAlignment="Top" Height="38" Width="177" Margin="10,628,0,0"/>
            <Border Margin="10,677,662,0">
                <Path Stroke="Black" StrokeThickness="1" Fill="Cyan" Stretch="Fill" Margin="0,4,-22,6">
                    <Path.Data>
                        <RectangleGeometry
            Rect="20,20,150,50" RadiusX="3" RadiusY="3" />
                    </Path.Data>
                </Path>
            </Border>
            <Label Content="-Odjava" FontSize="20" HorizontalAlignment="Left" VerticalAlignment="Top" Height="38" Width="177" Margin="10,690,0,0"/>
            <Label Content="Legenda simbola" FontSize="40" HorizontalAlignment="Left" Margin="321,4,0,0" VerticalAlignment="Top" Height="67" Width="332" FontWeight="Bold"/>
            <Button Content="Povratak"  Margin="0,672,417,36"  Width="100" Height="30" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" 
                Click="Povratak_Click" Background="WhiteSmoke" VerticalAlignment="Center" HorizontalAlignment="Right"/>
        </Grid>
    </StackPanel>
</UserControl>
