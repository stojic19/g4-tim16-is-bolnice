                </Path>
            </Border>
            <Label Content="-Obaveštenja" FontSize="20" HorizontalAlignment="Left" VerticalAlignment="Top" Height="38" Width="177" Margin="10,260,0,0"/>
            <Border Margin="10,313,662,367">
                <Path Stroke="Black" StrokeThickness="1" Fill="Cyan" Stretch="Fill" Margin="0,4,-22,6">
                    <Path.Data>
                        <RectangleGeometry
            Rect="20,20,150,50" RadiusX="3" RadiusY="3" />
                    </Path.Data>
                </Path>
            </Border>
            <Label Content="-Naplaćivanje usluge" FontSize="20" HorizontalAlignment="Left" VerticalAlignment="Top" Height="38" Width="198" Margin="10,322,0,0"/>
            <Border Margin="10,371,662,305">
                <Path Stroke="Black" StrokeThickness="1" Fill="Cyan" Stretch="Fill" Margin="0,4,-22,6">
                    <Path.Data>
                        <RectangleGeometry
            Rect="20,20,150,50" RadiusX="3" RadiusY="3" />
                    </Path.Data>
                </Path>
            </Border>
            <Label Content="-Stacionarno lečenje" FontSize="20" HorizontalAlignment="Left" VerticalAlignment="Top" Height="38" Width="198" Margin="10,383,0,0"/>
            <Border Margin="10,433,662,244">
                <Path Stroke="Black" StrokeThickness="1" Fill="Cyan" Stretch="Fill" Margin="0,4,-22,6">
                    <Path.Data>
                        <RectangleGeometry
            Rect="20,20,150,50" RadiusX="3" RadiusY="3" />
                    </Path.Data>
                </Path>
            </Border>
            <Label Content="-Transfer pacijenata" FontSize="20" HorizontalAlignment="Left" VerticalAlignment="Top" Height="38" Width="198" Margin="10,444,0,0"/>
            <Border Margin="10,494,662,183">
                <Path Stroke="Black" StrokeThickness="1" Fill="Cyan" Stretch="Fill" Margin="0,4,-22,6">
                    <Path.Data>
                        <RectangleGeometry
            Rect="20,20,150,50" RadiusX="3" RadiusY="3" />
                    </Path.Data>
                </Path>
            </Border>
            <Label Content="-Povratak na prethodnu stranu" FontSize="20" HorizontalAlignment="Left" VerticalAlignment="Top" Height="38" Width="283" Margin="10,506,0,0"/>
            <Border Margin="10,555,662,122">
                <Path Stroke="Black" StrokeThickness="1" Fill="Cyan" Stretch="Fill" Margin="0,4,-22,6">
                    <Path.Data>
                        <RectangleGeometry
